{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"],
      "@pages/*": ["pages/*"],
      "@contexts/*": ["contexts/*"],
      "@assets/*": ["assets/*"],
      "@hooks/*": ["hooks/*"],
      "@stories/*": ["stories/*"],
      "@styles/*": ["styles/*"],
      "@types/*": ["types/*"],
      "@service/*": ["service/*"],
      "@domainTypes/*": ["domainTypes/*"],
      "@enums/*": ["enums/*"],
      "@constants/*":["constants/*"]
    },
    "types": ["kakao.maps.d.ts", "jest"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.css"],
  "exclude": ["node_modules"]
}
