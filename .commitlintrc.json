{
  "extends": ["gitmoji"],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "build",
        "ci",
        "docs",
        "feat",
        "fix",
        "perf",
        "refactor",
        "revert",
        "style",
        "test",
        "chore",
        "wip",
        "config",
        "hotfix",
        "improve",
        "move",
        "add",
        "remove",
        "dep"
      ]
    ]
  }
}
